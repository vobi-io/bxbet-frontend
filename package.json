{
  "name": "BookingGenius-Front",
  "version": "1.0.0",
  "description": "BookingGenius Front",
  "main": "src/index.js",
  "repository": "git@github.com/btomashvili/BookingGenius-Front.git",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.production.config.js --progress --profile --colors",
    "build:test": "NODE_ENV=testing ./node_modules/.bin/webpack --config webpack.testing.config.js --progress --profile --colors",
    "start": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --profile --colors",
    "start:local": "NODE_ENV=development_local ./node_modules/.bin/webpack-dev-server --progress --profile --colors",
    "start:docker": "HOST=0.0.0.0 NODE_ENV=development_local ./node_modules/.bin/webpack-dev-server --progress --profile --colors --host 0.0.0.0 --port 9000",
    "start:test": "NODE_ENV=testing ./node_modules/.bin/webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": "webpack-dashboard -- webpack-dev-server --progress --profile --colors",
    "serv-dist": "http-server ./dist",
    "move:templates": "cp -R ./src/templates/* ./dist/templates",
    "format": "prettier-eslint --write \"src/**/*.js\""
  },
  "author": "Vobi.io",
  "license": "MIT",
  "keywords": [
    "ReactJS",
    "React hot loader",
    "React Router",
    "ESLint"
  ],
